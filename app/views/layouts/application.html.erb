<!DOCTYPE html>
<html>
  <head>
  <%= render 'layouts/head'%>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <body>
      <div class="container">
        <% flash.each do |message_type, message| %>
         <div class="alert alert-<%= message_type %>"><%= message %></div>
       <% end %>
        <%= yield %>
      </div>
      <%= render 'layouts/footer'%>
      <%= debug(params) if Rails.env.development? %>
    </body>
  </body>
</html>
